{% extends 'content_base.html.twig' %}

{% block manager_name %}
    Add an article
{% endblock %}

{% block content_body %}
    <h1>Add a category</h1>

    <div class="row">
        <div class="col-sm-12">
            {% include 'article_admin/articleForm.html.twig' %}
            <button type="submit" class="btn btn-success">Edit</button>
            <button type="reset" class="btn btn-info" onclick="javascritp:location.href='{{ path('admin_article_index') }}'">Cancel</button>
            {{ form_end(articleForm) }}
        </div>
    </div>
{% endblock %}